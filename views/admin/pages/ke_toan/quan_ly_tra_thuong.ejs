<%- include ../../layouts/header.ejs %>

    <!-- Begin page -->
    <div id="layout-wrapper">
        <header id="page-topbar">
    <div class="layout-width">
        <%- include ../../layouts/nav_header.ejs %>
    </div>
</header>
        <!-- ========== App Menu ========== -->
        <%- include ../../layouts/left_menu.ejs %>
        <!-- Left Sidebar End -->
        <!-- Vertical Overlay-->
        <div class="vertical-overlay"></div>

        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                                <h4 class="mb-sm-0">Trả thưởng hoa hồng</h4>

                                <div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">Quản lý</a></li>
                                        <li class="breadcrumb-item active">Trả thưởng</li>
                                    </ol>
                                </div>

                            </div>
                        </div> 
                    </div>
                    <!-- end page title -->

                    <div class="row">
                        <div class="col-5">
                            <div class="card">
                                <div class="card-header align-items-center d-flex">
                                    <h4 class="card-title mb-0 flex-grow-1">Trả thưởng sale <b></b> </h4>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="mb-6">
                                                <label for="mo_ta" class="form-label">Mã hợp đồng</label>
                                                <input type="text" class="form-control" placeholder="Mã hợp đồng" id="anh_dai_dien" name="anh_dai_dien">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="mb-6">
                                                <label for="mo_ta" class="form-label">Giá trị hợp đồng</label>
                                                <input type="text" class="form-control" placeholder="Giá trị hợp đồng" id="anh_dai_dien" name="anh_dai_dien">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="mb-6">
                                                <label for="mo_ta" class="form-label">Tên khách hàng</label>
                                                <input type="text" class="form-control" placeholder="Tên khách hàng" id="anh_dai_dien" name="anh_dai_dien">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="mb-6">
                                                <label for="mo_ta" class="form-label">CMND/CCCD</label>
                                                <input type="number" class="form-control" id="anh_dai_dien" name="anh_dai_dien">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="mb-6">
                                                <label for="mo_ta" class="form-label">Người hỗ trợ</label>
                                                <input type="text" class="form-control" id="anh_dai_dien" name="anh_dai_dien">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="mb-6">
                                                <label for="mo_ta" class="form-label">Chức vụ</label>
                                                <input type="text" class="form-control" id="anh_dai_dien" name="anh_dai_dien">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div> <!-- end col -->
                    </div>

                </div>
                <!-- container-fluid -->
            </div>
            <!-- End Page-content -->
        </div>
        <!-- end main content-->

    </div>
    <!-- END layout-wrapper -->



    <!--start back-to-top-->
    <button onclick="topFunction()" class="btn btn-danger btn-icon" id="back-to-top">
        <i class="ri-arrow-up-line"></i>
    </button>
    <!--end back-to-top-->

    <div class="customizer-setting d-none d-md-block">
        <div class="btn-info btn-rounded shadow-lg btn btn-icon btn-lg p-2" data-bs-toggle="offcanvas" data-bs-target="#theme-settings-offcanvas" aria-controls="theme-settings-offcanvas">
            <i class='mdi mdi-spin mdi-cog-outline fs-22'></i>
        </div>
    </div>
    <%- include ../../layouts/footer.ejs %>